{
  "name": "ext-think/encryption",
  "description": "think encryption package.",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "wanganlin",
      "email": "2797712@qq.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "topthink/framework": "^6.0.0",
    "topthink/think-helper": "^3.0.3"
  },
  "autoload": {
    "psr-4": {
      "think\\encryption\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "think": {
      "services": [
        "think\\encryption\\EncryptionService"
      ],
      "config": {
        "crypt": "src/config.php"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
